<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        tools:context=".base.BaseActivity">

        <cn.com.base.views.TitleBarView
            android:id="@+id/titlebar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48"
            android:background="@color/colorPrimaryDark"
            app:leftbtnName="发送1"
            app:leftdisplay="air"
            app:rightbtnName="发送2"
            app:rightdisplay="air"
            app:leftbtnColor="#FFFFFF"
            app:rightbtnColor="#FFFFFF"
            app:titlename="我是标题"
            />

        <cn.com.base.views.MultiLayout
            android:id="@+id/multiLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/container"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </cn.com.base.views.MultiLayout>
    </LinearLayout>
</layout>